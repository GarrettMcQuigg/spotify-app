    "axios": "^1.3.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonschema": "^1.4.1",
    "nodemon": "^2.0.21",
    "pg": "^8.10.0",
    "react-redux": "^8.0.5"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "eslint": "^8.36.0",
    "eslint-plugin-react": "^7.32.2"
  }
}
